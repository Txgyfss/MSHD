{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport request from \"@/utils/request\";\nexport default {\n  name: \"Register\",\n  data() {\n    return {\n      form: {}\n    };\n  },\n  // 页面加载的时候，做一些事情，在created里面\n  created() {},\n  // 定义一些页面上控件出发的事件调用的方法\n  methods: {\n    // 注册按钮点击事件入口\n    register() {\n      // 页面表单数据的输入校验\n      if (!this.form.username) {\n        this.$notify.error(\"请输入用户名\");\n        return;\n      }\n      if (!this.form.password) {\n        this.$notify.error(\"请输入密码\");\n        return;\n      }\n      if (this.form.password !== this.form.nmm) {\n        this.$notify.error(\"两次密码输入不一致\");\n        return;\n      }\n      if (!this.form.role) {\n        this.$notify.error(\"请选择角色\");\n        return;\n      }\n      // 调用后台注册接口\n      request.post('/register', this.form).then(res => {\n        if (res.code === '0') {\n          // 将用户信息保存一份到浏览器缓存里\n          this.$notify.success(\"注册成功！\");\n          this.$router.push(\"/login\");\n        } else {\n          this.$notify.error(res.msg);\n        }\n      });\n    },\n    navLogin() {\n      this.$router.push(\"/login\");\n    }\n  }\n};","map":{"version":3,"names":["request","name","data","form","created","methods","register","username","$notify","error","password","nmm","role","post","then","res","code","success","$router","push","msg","navLogin"],"sources":["src/views/Register.vue"],"sourcesContent":["\n<template>\n  <div class=\"login-container\">\n    <div style=\"display: flex\">\n      <el-form style=\"padding-left: 600px\">\n        <el-form-item class=\"form-box\" style=\"padding-top: 50px;border-radius: 10px\">\n          <el-form-item >\n            <el-input style=\"margin-bottom: 30px;\" size=\"large\" prefix-icon=\"el-icon-s-custom\" placeholder=\"请输入用户名\" v-model=\"form.username\"></el-input>\n          </el-form-item>\n          <el-form-item >\n            <el-input style=\"margin-bottom: 30px\" size=\"large\" show-password prefix-icon=\"el-icon-key\" placeholder=\"请输入密码\" v-model=\"form.password\"></el-input>\n          </el-form-item>\n          <el-form-item >\n            <el-input style=\"margin-bottom: 30px\" size=\"large\" show-password prefix-icon=\"el-icon-key\" placeholder=\"请输入确认密码\" v-model=\"form.nmm\"></el-input>\n          </el-form-item>\n          <el-form-item >\n            <el-select style=\"width: 100%;margin-bottom: 30px\" size=\"large\"  placeholder=\"请选择角色\" v-model=\"form.role\">\n              <el-option value=\"2\" label=\"用户\"></el-option>\n            </el-select>\n          </el-form-item>\n          <el-form-item>\n            <el-button style=\"width: 100%;height: 50px;background-color: #fdd85e;border-color: #fdd85e;color:white;font-size:20px;\" size=\"large\" @click=\"register\">注册</el-button>\n          </el-form-item>\n        </el-form-item>\n        <el-form-item style=\"background-color: #28a4fd; border-radius: 10px; display: flex; color: white; justify-content: center;align-items: center\">\n          <el-form-item class=\"regBox\">\n            <a href=\"javascript:void(0)\" @click=\"navLogin\">登录</a><span class=\"el-icon-right\"></span>\n          </el-form-item>\n\n        </el-form-item>\n\n      </el-form>\n    </div>\n  </div>\n</template>\n\n\n<script>\n\nimport request from \"@/utils/request\";\n\nexport default {\n  name: \"Register\",\n  data() {\n    return {\n      form: {}\n    }\n  },\n  // 页面加载的时候，做一些事情，在created里面\n  created() {\n  },\n  // 定义一些页面上控件出发的事件调用的方法\n  methods: {\n    // 注册按钮点击事件入口\n    register() {\n      // 页面表单数据的输入校验\n      if (!this.form.username) {\n        this.$notify.error(\"请输入用户名\");\n        return;\n      }\n      if (!this.form.password) {\n        this.$notify.error(\"请输入密码\");\n        return;\n      }\n      if (this.form.password !== this.form.nmm) {\n        this.$notify.error(\"两次密码输入不一致\");\n        return;\n      }\n      if (!this.form.role) {\n        this.$notify.error(\"请选择角色\");\n        return;\n      }\n      // 调用后台注册接口\n      request.post('/register', this.form).then(res => {\n        if(res.code === '0') {\n          // 将用户信息保存一份到浏览器缓存里\n          this.$notify.success(\"注册成功！\");\n          this.$router.push(\"/login\")\n        } else {\n          this  .$notify.error(res.msg);\n        }\n      });\n    },\n    navLogin() {\n      this.$router.push(\"/login\")\n    }\n  }\n}\n</script>\n<style scoped>\n.login-container {\n  height: 100vh;\n  overflow: hidden;\n  background-image: url(\"@/assets/imgs/login_bg.jpg\");\n  background-size: 100%;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n}\n\n.form-box {\n  width: 400px;\n  height: 425px;\n  padding: 20px 40px;\n  border-radius: 2px;\n  background-color: rgba(255,255,255,0);\n  opacity: 1;\n}\n/deep/.el-form-item {\n  margin-bottom: 5px;\n}\n/deep/.el-input__inner {\n  border-color: #ccc !important;\n  border-radius: 2px;\n  background-color: #ffffff !important;\n  color: #333;\n}\n/deep/.el-input__icon {\n  color: #666;\n}\n\na {\n  color: white;\n  font-size:16px;\n  text-indent: 12px;\n  text-decoration: none;\n  font-weight: bold;\n}\n.regBox {\n  font-size: 28px;\n  margin-bottom: 10px;\n  margin-top: 10px;\n  text-align: right;\n}\n</style>\n"],"mappings":";AAuCA,OAAAA,OAAA;AAEA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,IAAA;IACA;EACA;EACA;EACAC,QAAA,GACA;EACA;EACAC,OAAA;IACA;IACAC,SAAA;MACA;MACA,UAAAH,IAAA,CAAAI,QAAA;QACA,KAAAC,OAAA,CAAAC,KAAA;QACA;MACA;MACA,UAAAN,IAAA,CAAAO,QAAA;QACA,KAAAF,OAAA,CAAAC,KAAA;QACA;MACA;MACA,SAAAN,IAAA,CAAAO,QAAA,UAAAP,IAAA,CAAAQ,GAAA;QACA,KAAAH,OAAA,CAAAC,KAAA;QACA;MACA;MACA,UAAAN,IAAA,CAAAS,IAAA;QACA,KAAAJ,OAAA,CAAAC,KAAA;QACA;MACA;MACA;MACAT,OAAA,CAAAa,IAAA,mBAAAV,IAAA,EAAAW,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UACA;UACA,KAAAR,OAAA,CAAAS,OAAA;UACA,KAAAC,OAAA,CAAAC,IAAA;QACA;UACA,KAAAX,OAAA,CAAAC,KAAA,CAAAM,GAAA,CAAAK,GAAA;QACA;MACA;IACA;IACAC,SAAA;MACA,KAAAH,OAAA,CAAAC,IAAA;IACA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies"